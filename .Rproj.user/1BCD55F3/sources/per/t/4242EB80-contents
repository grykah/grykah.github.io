<span class="k">for</span> <span class="n">place</span> <span class="ow">in</span> <span class="n">df_priceDiff</span><span class="p">[</span><span class="s1">&#39;Borough&#39;</span><span class="p">]</span><span class="o">.</span><span class="n">unique</span><span class="p">():</span>
    <span class="n">priceRatios</span><span class="p">[</span><span class="n">place</span><span class="p">]</span> <span class="o">=</span> <span class="n">create_price_ratio</span><span class="p">(</span><span class="n">df_group</span><span class="p">,</span> <span class="n">place</span><span class="p">)</span>
