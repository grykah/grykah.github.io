<div class=" highlight hl-ipython3"><pre><span></span><span class="n">df_group</span> <span class="o">=</span> <span class="n">df</span><span class="o">.</span><span class="n">groupby</span><span class="p">([</span><span class="s1">&#39;Borough&#39;</span><span class="p">,</span><span class="s1">&#39;Year&#39;</span><span class="p">])</span><span class="o">.</span><span class="n">mean</span><span class="p">()</span>
<span class="c1">#print(df_group.loc[&#39;City of London&#39;])</span>
<span class="n">df_group</span><span class="o">.</span><span class="n">head</span><span class="p">(</span><span class="mi">30</span><span class="p">)</span>
<span class="c1">#df_group.dtypes</span>
<span class="c1">#print(df_group)</span>
<span class="c1">#df_group[&#39;Price&#39;].head()</span>

<span class="c1">#df_selectedYears = df.loc[df[&#39;Year&#39;] == 1995].append(df.loc[df[&#39;Year&#39;] == 2020])</span>
<span class="n">df1995</span> <span class="o">=</span> <span class="n">df</span><span class="o">.</span><span class="n">loc</span><span class="p">[</span><span class="n">df</span><span class="p">[</span><span class="s1">&#39;Year&#39;</span><span class="p">]</span> <span class="o">==</span> <span class="mi">1995</span><span class="p">]</span>  <span class="c1">#used lowest prices</span>
<span class="n">df1995</span> <span class="o">=</span> <span class="n">df1995</span><span class="o">.</span><span class="n">reset_index</span><span class="p">(</span><span class="n">drop</span><span class="o">=</span><span class="kc">True</span><span class="p">)</span>
<span class="n">df2019</span> <span class="o">=</span> <span class="n">df</span><span class="o">.</span><span class="n">loc</span><span class="p">[</span><span class="n">df</span><span class="p">[</span><span class="s1">&#39;Year&#39;</span><span class="p">]</span> <span class="o">==</span> <span class="mi">2018</span><span class="p">]</span>  <span class="c1">#switched to 2018 upon examination of above plot since 2018 has highest values than 2019/20, ultimately did not make a difference in the borough, but price diff was more significant</span>
<span class="n">df2019</span> <span class="o">=</span> <span class="n">df2019</span><span class="o">.</span><span class="n">reset_index</span><span class="p">(</span><span class="n">drop</span><span class="o">=</span><span class="kc">True</span><span class="p">)</span>

<span class="c1">#df1995.head(50)</span>
<span class="c1">#df2019.tail(50)</span>

<span class="n">df_priceDiff</span> <span class="o">=</span> <span class="n">pd</span><span class="o">.</span><span class="n">concat</span><span class="p">([</span><span class="n">df1995</span><span class="p">[</span><span class="s1">&#39;Price&#39;</span><span class="p">],</span> <span class="n">df2019</span><span class="p">[</span><span class="s1">&#39;Price&#39;</span><span class="p">]],</span> <span class="n">axis</span><span class="o">=</span><span class="mi">1</span><span class="p">)</span>
<span class="n">df_priceDiff</span> <span class="o">=</span> <span class="n">pd</span><span class="o">.</span><span class="n">concat</span><span class="p">([</span><span class="n">df1995</span><span class="p">[</span><span class="s1">&#39;Borough&#39;</span><span class="p">],</span> <span class="n">df_priceDiff</span><span class="p">],</span> <span class="n">axis</span><span class="o">=</span><span class="mi">1</span><span class="p">)</span>
<span class="n">colName</span> <span class="o">=</span> <span class="p">[</span><span class="s1">&#39;Borough&#39;</span><span class="p">,</span> <span class="s1">&#39;Price 1995&#39;</span><span class="p">,</span> <span class="s1">&#39;Price 2019&#39;</span><span class="p">]</span>
<span class="n">df_priceDiff</span><span class="o">.</span><span class="n">columns</span> <span class="o">=</span> <span class="n">colName</span>
<span class="n">df_priceDiff</span><span class="p">[</span><span class="s1">&#39;Price Difference&#39;</span><span class="p">]</span> <span class="o">=</span> <span class="p">(</span><span class="n">df_priceDiff</span><span class="p">[</span><span class="s1">&#39;Price 2019&#39;</span><span class="p">]</span><span class="o">.</span><span class="n">subtract</span><span class="p">(</span><span class="n">df_priceDiff</span><span class="p">[</span><span class="s1">&#39;Price 1995&#39;</span><span class="p">]))</span>
<span class="c1">#df_priceDiff = df2020[&#39;Price&#39;].subtract(df1995[&#39;Price&#39;], fill_value=0)</span>
<span class="c1">#df1995.dtypes</span>

<span class="c1">#print(df1995[&#39;Borough&#39;])</span>
<span class="c1">#print(df2020[&#39;Borough&#39;])</span>
<span class="nb">print</span><span class="p">(</span><span class="n">df_priceDiff</span><span class="p">)</span>

<span class="n">plt</span><span class="o">.</span><span class="n">plot</span><span class="p">(</span><span class="n">df_priceDiff</span><span class="p">[</span><span class="s1">&#39;Borough&#39;</span><span class="p">],</span> <span class="n">df_priceDiff</span><span class="p">[</span><span class="s1">&#39;Price Difference&#39;</span><span class="p">],</span> <span class="s1">&#39;b*&#39;</span><span class="p">)</span>
